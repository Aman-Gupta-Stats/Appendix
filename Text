import pandas as pd
import json

# Step 1: Load the Excel file
df = pd.read_excel('your_file.xlsx')  # Replace with your actual file path

# Step 2: Set 'gmdid' column as the key, and row values as dicts
result = {}
for _, row in df.iterrows():
    key = row['gmdid']  # this becomes the key
    value = row.drop(labels='gmdid').to_dict()  # rest of the row is the value
    result[key] = value

# Step 3: Save to JSON
with open('output.json', 'w') as f:
    json.dump(result, f, indent=4)
