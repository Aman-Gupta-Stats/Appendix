import pandas as pd

def to_excel_with_multi_header(df: pd.DataFrame, path: str, sheet_name: str = "Sheet1"):
    """
    Writes a DataFrame whose columns are a 2-level MultiIndex as a true two-row
    header in Excel, without writing Pandas' default index.
    """
    # 1) pull out our two header-rows
    lvl0 = list(df.columns.get_level_values(0))
    lvl1 = list(df.columns.get_level_values(1))

    # 2) start the writer
    with pd.ExcelWriter(path, engine="xlsxwriter") as writer:
        workbook  = writer.book
        worksheet = workbook.add_worksheet(sheet_name)
        writer.sheets[sheet_name] = worksheet

        # 3) write the data at row 2, no headers or index
        df.to_excel(
            writer,
            sheet_name=sheet_name,
            startrow=2,
            header=False,
            index=False
        )

        # 4) super-columns (level-0) on row 0, merging spans
        col = 0
        n = len(lvl0)
        while col < n:
            text = lvl0[col]
            # count how many in a row share this super-col
            span = 1
            while col + span < n and lvl0[col + span] == text:
                span += 1
            # merge from (row0, col) to (row0, col+spanâˆ’1)
            worksheet.merge_range(0, col, 0, col + span - 1, text)
            col += span

        # 5) sub-columns (level-1) on row 1
        worksheet.write_row(1, 0, lvl1)

        # 6) freeze panes so headers stay in view
        worksheet.freeze_panes(2, 0)